{
    "name": "eriri_stock",
    "version": "1.0.3",
    "manifest_version": 2,
    "description": "一个练手的小程序",
    "permissions": [
        "tabs",
        "http://*/",
        "https://*/"
    ],
    "icons":{
        "16":"icon.jpeg",
        "48":"icon.jpeg",
        "128":"icon.jpeg"
    },
    "browser_action": {
        "default_icon": "icon.jpeg",
        "default_title": "eriri_stock",
        "default_popup": "index.html"
    }
}
